{
  "manifest_version": 2,
  "name": "FamilySearch Enhancer",
  "version": "0.1",

  "description": "Enhances FamilySearch's functionality",

  "content_scripts": [
    {
      "matches": ["*://www.familysearch.org/indexing/batch/*"],
      "js": ["content_scripts/indexing.js"]
    }
  ],

  "web_accessible_resources": [
    "page_scripts/*"
  ]
}
